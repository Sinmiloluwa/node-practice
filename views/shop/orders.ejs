<%- include('../includes/head.ejs') %>
<body>
    <%- include('../includes/nav.ejs') %>
    <main>
        <% if (orders.length <= 0) { %>
            <h1>Nothing Here</h1>
        <% } else { %>
            <ul>
                <% orders.forEach(order => { %>
                    <li>
                        <h1># <%= order.id %></h1>
                        <ul>
                            <% order.products.forEach(product => { %>
                                <li><%= product.title %> {<%= product.orderItem.quantity %>}</li>
                            <% }) %>
                        </ul>
                    </li>
                <% }) %>
            </ul>
        <% } %>
        <h1>Orders</h1>
    </main>
</body>
